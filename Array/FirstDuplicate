
import java.util.HashSet;
public class FirstDuplicate {
    public static void main(String[] args) {
        
    
    int[]arr={1,2,1,3,4};

    HashSet<Integer> set=new HashSet<>();                    

    for(int num:arr){
        if(set.contains(num)){
            System.out.println("first duplicate is  : " +num);
            return;
        }
        set.add(num);
    }

}}
